<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Agronautes | {% endblock %}</title>
		
        {% block stylesheets %}
		<link rel="icon" type="image/x-icon" href="{{ asset('css/bootstrap.min.css') }}" />
		{% endblock %}
		
		{% block fonts %}
		<link rel="icon" type="image/x-icon" href="{{ asset('css/font-awesome.min.css') }}" />
		{% endblock %}

		
		{% block favicon %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
		{% endblock %}
    </head>
	
    <body>
        {% block body %}
		{% endblock %}
		
        {% block javascripts %}
		<link rel="icon" type="image/x-icon" href="{{ asset('js/bootstrap.min.js') }}" />
		<link rel="icon" type="image/x-icon" href="{{ asset('js/jquery.min.js') }}" />
		{% endblock %}
    </body>
</html>
